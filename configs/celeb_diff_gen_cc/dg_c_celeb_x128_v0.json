{
    "globals": {
        "project": "Celeb_Generation",
        "author": "Marc",
        "parent": "../keys.json"
    },
    "transforms": {
        "resize_norm": [
            "Float32",
            {
                "Resize_HW": {
                    "h": 128,
                    "w": 128
                }
            },
            {
                "ReRange": {
                    "in_min": 0,
                    "in_max": 255,
                    "out_min": -1,
                    "out_max": 1
                }
            }
        ],
        "norm_to_rgb": [
            {
                "ReRange": {
                    "in_min": -1,
                    "in_max": 1,
                    "out_min": 0,
                    "out_max": 255
                }
            },
            {
                "CLAMP_abs": {
                    "lower": 0,
                    "upper": 255
                }
            },
            "Uint8"
        ]
    },
    "data": {
        "celeb_HDF": {
            "processor": "~/dev/fdq_diffusion/hdf_preparator_celeb.py",
            "args": {
                "base_path": "~/data/ML_data/celeba_hq_256/celeb_train_and_test/",
                "file_extension": "hdf",
                "val_from_train_ratio": 0.05,
                "subset_train": 1.0,
                "subset_val": 1.0,
                "subset_test": 1.0,
                "pin_memory": true,
                "num_workers": 4,
                "train_batch_size": 16,
                "val_batch_size": 8,
                "test_batch_size": 1,
                "shuffle_train": true,
                "shuffle_val": false,
                "shuffle_test": false,
                "drop_last": true,
                "hdf_ds_key_request": [
                    {
                        "*": "resize_norm"
                    }
                ],
                "hdf_ds_key_request_test": [
                    {
                        "*": "resize_norm"
                    }
                ]
            },
            "caching": {
                "enabled": true,
                "cache_dir": "/scratch/fdq_cache/",
                "shuffle_train": true,
                "shuffle_val": false,
                "shuffle_test": false,
                "pin_memory": false,
                "num_workers": 4,
                "compress_cache": true
            }
        }
    },
    "models": {
        "ccUNET": {
            "class_name": "chuchichaestli.models.unet.unet.UNet",
            "args": {
                "dimensions": 2,
                "in_channels": 3,
                "out_channels": 3,
                "n_channels": 64,
                "num_blocks_per_level": 1,
                "down_block_types": [
                    "DownBlock",
                    "DownBlock",
                    "DownBlock",
                    "AttnDownBlock",
                    "AttnDownBlock"
                ],
                "up_block_types": [
                    "UpBlock",
                    "UpBlock",
                    "UpBlock",
                    "AttnUpBlock",
                    "AttnUpBlock"
                ],
                "block_out_channel_mults": [
                    1,
                    2,
                    2,
                    2,
                    2
                ],
                "time_embedding": true,
                "time_channels": 512
            },
            "optimizer": {
                "class_name": "torch.optim.Adam",
                "args": {
                    "lr": 0.00005
                }
            },
            "lr_scheduler": {
                "class_name": "torch.optim.lr_scheduler.StepLR",
                "args": {
                    "step_size": 20,
                    "gamma": 0.95
                }
            }
        }
    },
    "train": {
        "path": "~/dev/fdq_diffusion/diffusion_chuchichaestli.py",
        "args": {
            "epochs": 250,
            "use_GPU": true,
            "use_AMP": true,
            "accumulate_grad_batches": 1,
            "early_stop_val_loss": 50,
            "early_stop_train_loss": 50,
            "early_stop_nan": 5,
            "diffusion_scheduler": "linear",
            "diffusion_nb_steps": 800,
            "diffusion_shd_beta_start": 0.0001,
            "diffusion_shd_beta_end": 0.02,
            "diffusion_nb_plot_steps": 10,
            "dataloader_name": "celeb_HDF",
            "model_name": "ccUNET"
        }
    },
    "losses": {
        "MAE": {
            "class_name": "torch.nn.L1Loss"
        },
        "MSE": {
            "class_name": "torch.nn.MSELoss"
        }
    },
    "store": {
        "results_path": "~/data/ML_data/results",
        "img_exp_nb": 4,
        "save_last_model": true,
        "save_best_val_model": false,
        "save_best_train_model": true,
        "checkpoint_frequency": 2,
        "use_tensorboard": false,
        "use_wandb": true,
        "wandb_project": "diff_gen_celeb_x128",
        "wandb_entity": "stmd",
        "img_exp_transform": "norm_to_rgb"
    },
    "test": {
        "processor": "~/dev/fdq_diffusion/diffusion_chuchichaestli.py",
        "test_model": "best",
        "args": {
            "nb_test_samples": 10
        }
    },
    "slurm_cluster": {
        "fdq_test_repo": false,
        "fdq_version": "0.0.72",
        "python_env_module": "python/3.12.4",
        "uv_env_module": "uv/0.6.12",
        "cuda_env_module": "cuda/12.8.0",
        "scratch_results_path": "/scratch/fdq_results/",
        "scratch_data_path": "/scratch/fdq_data/",
        "log_path": "~/dev/fdq_diffusion/slurm_log",
        "job_time": 1400,
        "stop_grace_time": 15,
        "ntasks": 1,
        "cpus_per_task": 8,
        "gres": "gpu:a100sxm:1",
        "mem": "120G",
        "partition": "gpu",
        "account": "cai_ivs",
        "run_train": true,
        "run_test": true,
        "auto_resubmit": true,
        "resume_chpt_path": null,
        "additional_pip_packages": [
            "h5py",
            "chuchichaestli==0.2.14"
        ]
    }
}