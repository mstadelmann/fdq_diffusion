{
    "globals": {
        "project": "Celeb_VAE",
        "author": "Marc"
    },
    "transforms": {
        "resize_norm": [
            "Float32",
            {
                "Resize_HW": {
                    "h": 128,
                    "w": 128
                }
            },
            {
                "ReRange": {
                    "in_min": 0,
                    "in_max": 255,
                    "out_min": -1,
                    "out_max": 1
                }
            }
        ],
        "norm_to_rgb": [
            {
                "ReRange": {
                    "in_min": -1,
                    "in_max": 1,
                    "out_min": 0,
                    "out_max": 255
                }
            },
            {
                "CLAMP_abs": {
                    "lower": 0,
                    "upper": 255
                }
            },
            "Uint8"
        ]
    },
    "data": {
        "celeb_HDF": {
            "processor": "~/dev/fdq_diffusion/hdf_preparator_v2.py",
            "args": {
                "base_path": "~/data/ML_data/celeba_hq_256/celeb_train_and_test/",
                "file_extension": "hdf",
                "val_from_train_ratio": 0.1,
                "subset_train": 1.0,
                "subset_val": 1.0,
                "subset_test": 1.0,
                "pin_memory": true,
                "num_workers": 4,
                "train_batch_size": 32,
                "val_batch_size": 32,
                "test_batch_size": 1,
                "shuffle_train": true,
                "shuffle_val": false,
                "shuffle_test": false,
                "drop_last": true,
                "hdf_ds_key_request": [
                    {
                        "*": "resize_norm"
                    }
                ],
                "hdf_ds_key_request_test": [
                    {
                        "*": "resize_norm"
                    }
                ]
            }
        }
    },
    "models": {
        "monaivae": {
            "class_name": "monai.networks.nets.AutoencoderKL",
            "args": {
                "spatial_dims": 2,
                "in_channels": 3,
                "out_channels": 3,
                "latent_channels": 3,
                "channels": [
                    64,
                    128,
                    256
                ],
                "num_res_blocks": 2,
                "norm_num_groups": 32,
                "norm_eps": 1e-06,
                "attention_levels": [
                    false,
                    false,
                    false
                ],
                "with_encoder_nonlocal_attn": false,
                "with_decoder_nonlocal_attn": false
            },
            "optimizer": {
                "class_name": "torch.optim.Adam",
                "args": {
                    "lr": 0.0001
                }
            },
            "lr_scheduler": {
                "class_name": "torch.optim.lr_scheduler.StepLR",
                "args": {
                    "step_size": 20,
                    "gamma": 0.95
                }
            }
        }
    },
    "train": {
        "path": "~/dev/fdq_diffusion/vae.py",
        "args": {
            "epochs": 50,
            "use_GPU": true,
            "use_AMP": true,
            "accumulate_grad_batches": 1,
            "early_stop_val_loss": 20,
            "early_stop_train_loss": 20,
            "early_stop_nan": 5,
            "kl_loss_weight": 1e-6,
            "dataloader_name": "celeb_HDF",
            "model_name": "monaivae"
        }
    },
    "losses": {
        "MAE": {
            "class_name": "torch.nn.L1Loss"
        }
    },
    "store": {
        "results_path": "~/data/ML_data/results",
        "img_exp_nb": 8,
        "save_last_model": true,
        "save_best_val_model": true,
        "save_best_train_model": false,
        "checkpoint_frequency": 5,
        "use_tensorboard": false,
        "use_wandb": true,
        "wandb_project": "celeb_vae_x128",
        "wandb_entity": "stmd",
        "wandb_key": "126aea51c74a8e5533f79411f171a1a2860a2e3a",
        "img_exp_transform": "norm_to_rgb"
    },
    "test": {
        "processor": "~/dev/fdq_diffusion/vae.py",
        "test_model": "best",
        "args": {
            "nb_test_samples": 20,
            "nb_export_samples": 10
        }
    },
    "slurm_cluster": {
        "fdq_version": "0.0.60",
        "python_env_module": "python/3.12.4",
        "uv_env_module": "uv/0.6.12",
        "scratch_results_path": "/scratch/fdq_results/",
        "scratch_data_path": "/scratch/fdq_data/",
        "log_path": "~/dev/fdq_diffusion/slurm_log",
        "job_time": 3000,
        "stop_grace_time": 15,
        "ntasks": 1,
        "cpus_per_task": 8,
        "gres": "gpu:a100sxm:1",
        "mem": "120G",
        "partition": "gpu",
        "account": "cai_ivs",
        "run_train": true,
        "run_test": true,
        "auto_resubmit": true,
        "resume_chpt_path": null,
        "additional_pip_packages": [
            "h5py"
        ]
    }
}