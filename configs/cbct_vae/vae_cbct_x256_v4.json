{
    "globals": {
        "project": "CBCT_VAE",
        "author": "Marc",
        "parent": "vae_cbct_x256.json"
    },
    "data": {
        "ctrate": {
            "args": {
                "train_batch_size": 1,
                "val_batch_size": 1
            }
        }
    },
    "train": {
        "args": {
            "accumulate_grad_batches": 4
        }
    },
    "models": {
        "monaivae": {
            "class_name": "monai.networks.nets.AutoencoderKL",
            "args": {
                "spatial_dims": 3,
                "in_channels": 1,
                "out_channels": 1,
                "latent_channels": 1,
                "channels": [
                    128,
                    256,
                    512
                ],
                "num_res_blocks": 2,
                "norm_num_groups": 32,
                "norm_eps": 1e-06,
                "attention_levels": [
                    false,
                    false,
                    false
                ],
                "with_encoder_nonlocal_attn": false,
                "with_decoder_nonlocal_attn": false
            }
        }
    }
}