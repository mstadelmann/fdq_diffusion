{
    "globals": {
        "project": "Celeb_VAE",
        "author": "Marc"
    },
    "data": {
        "celeb_HDF": {
            "processor": "~/dev/fdq_diffusion/hdf_preparator.py",
            "args": {
                "base_path": "~/data/ML_data/celeba_hq_256/celeb_train_and_test/",
                "file_extension": "hdf",
                "val_from_train_ratio": 0.1,
                "subset_train": 0.1,
                "subset_val": 1.0,
                "subset_test": 1.0,
                "pin_memory": true,
                "num_workers": 4,
                "train_batch_size": 16,
                "val_batch_size": 16,
                "test_batch_size": 1,
                "shuffle_train": true,
                "shuffle_val": false,
                "shuffle_test": false,
                "drop_last": true,
                "transforms": {
                    "resize_h": 128,
                    "resize_w": 128,
                    "in_min": 0,
                    "in_max": 255,
                    "norm_min": -1,
                    "norm_max": 1,
                    "export_lower_perc_cutoff": 0.005,
                    "export_upper_perc_cutoff": 0.995
                }
            }
        }
    },
    "models": {
        "monaivae": {
            "class_name": "monai.networks.nets.AutoencoderKL",
            "trained_model_path": "/cluster/home/stmd/data/ML_data/results/Celeb_VAE/celeb_vae_x128/20250523_09_30_18__romantic_pare__34673/last_monaivae_e4.fdqm",
            "freeze": true,
            "args": {
                "spatial_dims": 2,
                "in_channels": 3,
                "out_channels": 3,
                "latent_channels": 3,
                "channels": [
                    64,
                    128,
                    256
                ],
                "num_res_blocks": 2,
                "norm_num_groups": 32,
                "norm_eps": 1e-06,
                "attention_levels": [
                    false,
                    false,
                    false
                ],
                "with_encoder_nonlocal_attn": false,
                "with_decoder_nonlocal_attn": false
            }
        },
        "ccUNET": {
            "class_name": "chuchichaestli.models.unet.unet.UNet",
            "args": {
                "dimensions": 2,
                "in_channels": 3,
                "out_channels": 3,
                "n_channels": 128,
                "num_layers_per_block": 2,
                "down_block_types": [
                    "DownBlock",
                    "DownBlock",
                    "DownBlock",
                    "DownBlock"
                ],
                "up_block_types": [
                    "UpBlock",
                    "UpBlock",
                    "UpBlock",
                    "UpBlock"
                ],
                "block_out_channel_mults": [
                    1,
                    1,
                    2,
                    2
                ],
                "time_embedding": true,
                "time_channels": 512
            },
            "optimizer": {
                "class_name": "torch.optim.Adam",
                "args": {
                    "lr": 0.0001
                }
            },
            "lr_scheduler": {
                "class_name": "torch.optim.lr_scheduler.StepLR",
                "args": {
                    "step_size": 20,
                    "gamma": 0.95
                }
            }
        }
    },
    "train": {
        "path": "~/dev/fdq_diffusion/latent_diffusion.py",
        "args": {
            "epochs": 20,
            "use_GPU": true,
            "use_AMP": true,
            "accumulate_grad_batches": 1,
            "early_stop_val_loss": 20,
            "early_stop_train_loss": 20,
            "early_stop_nan": 5,
            "diffusion_scheduler": "linear",
            "diffusion_nb_steps": 800,
            "diffusion_shd_beta_start": 0.0001,
            "diffusion_shd_beta_end": 0.02,
            "diffusion_nb_plot_steps": 10
        }
    },
    "losses": {
        "MAE": {
            "class_name": "torch.nn.L1Loss"
        }
    },
    "store": {
        "results_path": "~/data/ML_data/results",
        "img_exp_nb": 8,
        "save_last_model": true,
        "save_best_val_model": true,
        "save_best_train_model": true,
        "checkpoint_frequency": 5,
        "use_tensorboard": false,
        "use_wandb": true,
        "wandb_project": "celeb_lat_diff",
        "wandb_entity": "stmd",
        "wandb_key": "126aea51c74a8e5533f79411f171a1a2860a2e3a"
    },
    "test": {
        "processor": "~/dev/fdq_diffusion/vae.py",
        "test_model": "best",
        "args": {
            "nb_test_samples": 100,
            "nb_export_samples": 10
        }
    },
    "slurm_cluster": {
        "fdq_version": "0.0.10",
        "python_env_module": "python/3.12.4",
        "uv_env_module": "uv/0.6.12",
        "scratch_results_path": "/scratch/fdq_results/",
        "scratch_data_path": "/scratch/fdq_data/",
        "log_path": "~/dev/fdq_diffusion/slurm_log",
        "job_time": 1400,
        "stop_grace_time": 15,
        "ntasks": 1,
        "cpus_per_task": 8,
        "gres": "gpu:1",
        "mem": "120G",
        "partition": "gpu",
        "account": "cai_ivs",
        "run_train": true,
        "run_test": true,
        "auto_resubmit": true,
        "resume_chpt_path": null,
        "additional_pip_packages": [
            "h5py"
        ]
    }
}