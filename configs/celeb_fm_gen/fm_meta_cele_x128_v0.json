{
    "globals": {
        "project": "Celeb_FM_Generation",
        "author": "Marc",
        "parent": "../keys.json"
    },
    "transforms": {
        "resize_norm": [
            "Float32",
            {
                "Resize_HW": {
                    "h": 128,
                    "w": 128
                }
            },
            {
                "ReRange": {
                    "in_min": 0,
                    "in_max": 255,
                    "out_min": 0,
                    "out_max": 1
                }
            }
        ],
        "norm_to_rgb": [
            {
                "ReRange": {
                    "in_min": -1,
                    "in_max": 1,
                    "out_min": 0,
                    "out_max": 255
                }
            },
            {
                "CLAMP_abs": {
                    "lower": 0,
                    "upper": 255
                }
            },
            "Uint8"
        ]
    },
    "data": {
        "celeb_HDF": {
            "processor": "~/dev/fdq_diffusion/hdf_preparator_v2.py",
            "args": {
                "base_path": "~/data/ML_data/celeba_hq_256/celeb_train_and_test/",
                "file_extension": "hdf",
                "val_from_train_ratio": 0.05,
                "subset_train": 1.0,
                "subset_val": 1.0,
                "subset_test": 1.0,
                "pin_memory": true,
                "num_workers": 4,
                "train_batch_size": 16,
                "val_batch_size": 8,
                "test_batch_size": 1,
                "shuffle_train": true,
                "shuffle_val": false,
                "shuffle_test": false,
                "drop_last": true,
                "hdf_ds_key_request": [
                    {
                        "*": "resize_norm"
                    }
                ],
                "hdf_ds_key_request_test": [
                    {
                        "*": "resize_norm"
                    }
                ]
            }
        }
    },
    "models": {
        "metaUNET": {
            "path": "~/dev/fdq_diffusion/meta_unet.py",
            "class_name": "UNetModel",
            "args": {
                "in_channels": 3,
                "model_channels": 128,
                "out_channels": 3,
                "num_res_blocks": 4,
                "attention_resolutions": [
                    2
                ],
                "dropout": 0.3,
                "channel_mult": [
                    2,
                    2,
                    2
                ],
                "conv_resample": false,
                "dims": 2,
                "num_classes": null,
                "use_checkpoint": false,
                "num_heads": 1,
                "num_head_channels": -1,
                "num_heads_upsample": -1,
                "use_scale_shift_norm": true,
                "resblock_updown": false,
                "use_new_attention_order": true,
                "with_fourier_features": false
            },
            "optimizer": {
                "class_name": "torch.optim.Adam",
                "args": {
                    "lr": 0.0001
                }
            },
            "lr_scheduler": {
                "class_name": "torch.optim.lr_scheduler.StepLR",
                "args": {
                    "step_size": 20,
                    "gamma": 0.95
                }
            }
        }
    },
    "train": {
        "path": "~/dev/fdq_diffusion/meta_flow_match_train.py",
        "args": {
            "epochs": 150,
            "use_GPU": true,
            "use_AMP": true,
            "accumulate_grad_batches": 1,
            "early_stop_val_loss": 50,
            "early_stop_train_loss": 20,
            "early_stop_nan": 5,
            "dataloader_name": "celeb_HDF",
            "model_name": "metaUNET"
        }
    },
    "losses": {
        "MSE": {
            "class_name": "torch.nn.MSELoss"
        }
    },
    "store": {
        "results_path": "~/data/ML_data/results",
        "img_exp_nb": 4,
        "save_last_model": true,
        "save_best_val_model": true,
        "save_best_train_model": true,
        "checkpoint_frequency": 4,
        "use_tensorboard": false,
        "use_wandb": true,
        "wandb_project": "fm_m_celeb_x128",
        "wandb_entity": "stmd",
        "img_exp_transform": "norm_to_rgb"
    },
    "test": {
        "processor": "~/dev/fdq_diffusion/meta_flow_match_train.py",
        "test_model": "last",
        "args": {
            "nb_test_samples": 10
        }
    },
    "slurm_cluster": {
        "fdq_version": "0.0.72",
        "python_env_module": "python/3.12.4",
        "uv_env_module": "uv/0.6.12",
        "scratch_results_path": "/scratch/fdq_results/",
        "scratch_data_path": "/scratch/fdq_data/",
        "log_path": "~/dev/fdq_diffusion/slurm_log",
        "job_time": 1400,
        "stop_grace_time": 15,
        "ntasks": 1,
        "cpus_per_task": 8,
        "cpus_per_task_test": 8,
        "gres": "gpu:1",
        "gres_test": "gpu:1",
        "mem": "100G",
        "mem_test": "100G",
        "partition": "gpu_top",
        "account": "cai_ivs",
        "run_train": false,
        "run_test": true,
        "auto_resubmit": true,
        "resume_chpt_path": null,
        "additional_pip_packages": [
            "h5py",
            "flow_matching==1.0.10"
        ]
    }
}